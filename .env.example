# Supabase Configuration - REQUIRED
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Project ID - REQUIRED
VITE_PROJECT_ID=your-project-id

# Service Role Key (for server-side operations only) - OPTIONAL
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
VITE_SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Supabase Access Token (for CLI operations) - OPTIONAL
SUPABASE_ACCESS_TOKEN=your-access-token

# Evolution API Configuration - OPTIONAL
VITE_EVOLUTION_API_URL=
VITE_EVOLUTION_API_KEY=
VITE_EVOLUTION_INSTANCE_NAME=
VITE_EVOLUTION_API_ENABLED=false

# Resend API Key (for email service) - OPTIONAL
VITE_RESEND_API_KEY=your-resend-key